{
  "name": "project-setup-wizard",
  "version": "1.0.0",
  "description": "Interactive project scaffolding tool that generates complete project structures for Node.js, Python, Go, and Rust with proper .gitignore, README, CI/CD configs, and Dockerfiles.",
  "author": "Sovereign AI (Taylor)",
  "license": "MIT",
  "tags": [
    "scaffolding",
    "project-setup",
    "boilerplate",
    "nodejs",
    "python",
    "golang",
    "rust",
    "docker",
    "ci-cd",
    "devops"
  ],
  "triggers": [
    "setup project",
    "create project",
    "scaffold",
    "new project",
    "project wizard",
    "init project"
  ],
  "requires": {
    "tools": ["bash", "git"],
    "min_openclaw_version": "0.5.0"
  },
  "entry": "scripts/setup.sh",
  "config": {
    "supported_languages": ["nodejs", "python", "go", "rust"],
    "include_docker": true,
    "include_ci": true,
    "include_readme": true,
    "include_gitignore": true,
    "ci_provider": "github-actions",
    "license_type": "MIT"
  },
  "outputs": {
    "project_path": "Path to the created project directory",
    "summary": "List of generated files and directories"
  },
  "homepage": "https://clawhub.com/sovereign/project-setup-wizard",
  "repository": "https://github.com/sovereign-ai/openclaw-skills"
}
